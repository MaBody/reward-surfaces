name: reward-surfaces
channels:
  - pytorch
  - nvidia
  - conda-forge
dependencies:
  - python=3.10
  - pip
  - numpy=2.2.6
  - pandas=2.2.3
  - scipy=1.15.3
  - matplotlib=3.10.3
  - scikit-learn  # inferred, replace with exact if needed
  - jupyterlab  # inferred from jupyter_core, jupyter_client, ipykernel
  - ipython=9.3.0
  - ipykernel=6.29.5
  - pillow=11.2.1
  - tqdm=4.67.1
  - traitlets=5.14.3
  - tornado=6.5.1
  - pyzmq=26.4.0
  - typing_extensions=4.14.0
  - packaging=25.0
  - pyyaml=6.0.2
  - setuptools=80.9.0
  - wheel
  - fonttools=4.58.1
  - jinja2=3.1.6
  - prompt_toolkit=3.0.51
  - ptyprocess=0.7.0
  - psutil=7.0.0
  - six=1.17.0
  - pyparsing=3.2.3
  - cycler=0.12.1
  - kiwisolver=1.4.8
  - jsonschema=4.24.0
  - executing=2.2.0
  - asttokens=3.0.0
  - comm=0.2.2
  - debugpy=1.8.14
  - filelock=3.18.0
  - fastjsonschema=2.21.1
  - fsspec=2025.5.1
  - networkx=3.5
  - nest-asyncio=1.6.0
  - stack-data=0.6.3
  - jsonschema-specifications=2025.4.1
  - jupyter_client=8.6.3
  - jupyter_core=5.8.1
  - parso=0.8.4
  - pure_eval=0.2.3
  - platformdirs=4.3.8
  - referencing=0.36.2
  - rpds-py=0.25.1
  - sb3_contrib=2.6.0
  - stable_baselines3=2.6.0
  - sympy=1.14.0
  - triton=3.3.0
  # NVIDIA CUDA libraries pinned as you listed
  - nvidia-cublas-cu12=12.6.4.1
  - nvidia-cuda-cupti-cu12=12.6.80
  - nvidia-cuda-nvrtc-cu12=12.6.77
  - nvidia-cuda-runtime-cu12=12.6.77
  - nvidia-cudnn-cu12=9.5.1.17
  - nvidia-cufft-cu12=11.3.0.4
  - nvidia-cufile-cu12=1.11.1.6
  - nvidia-curand-cu12=10.3.7.77
  - nvidia-cusolver-cu12=11.7.1.2
  - nvidia-cusparse-cu12=12.5.4.2
  - nvidia-cusparselt-cu12=0.6.3
  - nvidia-nccl-cu12=2.26.2
  - nvidia-nvjitlink-cu12=12.6.85
  - nvidia-nvtx-cu12=12.6.77

  # PyTorch from pytorch channel (CPU-only; GPU might require manual build)
  - pytorch=2.0.*
  - torchvision=0.15.*
  - torchaudio=2.0.*

  # Add CPU-only fallback package in case CUDA libs don't fully link
  - cpuonly

  # Packages only available on PyPI, installed via pip
  - pip:
      - atari-py==0.2.9
      - opencv-python==4.11.0.86
      - Farama-Notifications==0.0.4
      - ipython_pygments_lexers==1.1.1
      - jedi==0.19.2
      - matplotlib-inline==0.1.7
      - mpmath==1.3.0
      - narwhals==1.41.0
      - packaging==25.0
      - pathlib==1.0.1
      - plotly==6.1.2
      - pexpect==4.9.0
      - pygments==2.19.1
      - python-dateutil==2.9.0.post0
      - pytz==2025.2
      - referencING==0.36.2
      - rpds-py==0.25.1
      - six==1.17.0
      - wcwidth==0.2.13

